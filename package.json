{
  "name": "bData",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --transpile-only ./src/index.ts",
    "dev:typecheck": "tsc --watch --noEmit",
    "start": "cross-env NODE_ENV=${NODE_ENV:-development} dotenv-flow -- node .",
    "build:reflection": "cross-env NEXUS_SHOULD_EXIT_AFTER_REFLECTION=true node -r dotenv-flow/config --experimental-specifier-resolution=node --loader ts-node/esm ./src/api/schema.ts",
    "build": "yarn build:reflection && tsc",
    "lint": "eslint \"./**/*.{ts,tsx,json}\"",
    "format": "prettier --write \"./**/*.{ts,tsx}\"",
    "prisma": "cross-env NODE_ENV=${NODE_ENV:-development} dotenv-flow -- prisma",
    "tinker": "cross-env NODE_ENV=${NODE_ENV:-development} dotenv-flow -- ts-node tinker.ts",
    "migrate:dev": "yarn prisma migrate dev",
    "migrate:dev:create": "yarn prisma migrate dev --create-only",
    "migrate:deploy": "yarn prisma migrate deploy",
    "migrate:reset": "yarn prisma migrate reset",
    "migrate:resolve": "yarn prisma migrate resolve",
    "prisma:generate": "yarn prisma generate",
    "prisma:generate:watch": "yarn prisma generate --watch",
    "prisma:seed": "yarn prisma db seed --preview-feature",
    "prisma:studio": "yarn prisma studio"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "dependencies": {
    "@prisma/client": "^2.28.0",
    "apollo-server-express": "^3.0.0",
    "binance-api-node": "^0.11.5",
    "cors": "^2.8.5",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.3",
    "graphql": "^15.5.1",
    "ioredis": "^4.27.6",
    "nexus": "^1.1.0",
    "nexus-prisma": "^0.30.0",
    "nexus-shield": "^2.0.0",
    "rate-limiter-flexible": "^2.2.1",
    "redis-semaphore": "^4.1.0",
    "subscriptions-transport-ws": "^0.9.19"
  },
  "devDependencies": {
    "@types/dotenv-flow": "^3.1.1",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^4.26.5",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "cross-env": "^7.0.2",
    "dotenv-flow-cli": "^1.0.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "prettier": "^2.2.1",
    "prisma": "^2.28.0",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.3",
    "typescript": "^4.4.0-dev.20210608"
  }
}
